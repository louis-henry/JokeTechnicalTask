{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Enrich": [
      "WithThreadId",
      "WithThreadName"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yy-MM-dd HH:mm:ss} {ThreadName}:{ThreadId}][{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}